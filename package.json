{
  "name": "server-names",
  "version": "0.0.0-semantically-released",
  "description": "Get Random server-names ... based on north mythology and star-gate",
  "main": "dist/index.js",
  "repository": "git@github.com:renemeye/server-names.git",
  "author": "Ren√© Meye <rene.meye@vesputi.com> (https://themobilitybox.com/)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "mocha": "8.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "^17.3.7"
  },
  "scripts": {
    "watch:test": "npm test -- --watch --watch-extensions",
    "test": "mocha src/index.test.js --require babel-core/register",
    "cover": "nyc npm test",
    "check-coverage": "nyc check-coverage --statements 100 --lines 100 --branches 100 --functions 100",
    "report-coverage": "cat coverage/lcov.info | codecov",
    "semantic-release": "semantic-release",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "commit": "cz"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "babel": {
    "presets": ["es2015", "stage-2"]
  }
}
