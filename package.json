{
  "name": "server-names",
  "version": "0.0.0-semantically-released",
  "description": "Get Random server-names ... based on north mythology and star-gate",
  "main": "src/index.js",
  "repository": "git@github.com:renemeye/server-names.git",
  "author": "Ren√© Meye <rene.meye@vesputi.com> (https://themobilitybox.com/)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "mocha": "8.2.1",
    "semantic-release": "^17.3.7"
  },
  "scripts": {
    "test": "mocha src/index.test.js -w",
    "test:once": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "check-coverage": "istanbul check-coverage --statements 100 --lines 100 --branches 100 --functions 100",
    "semantic-release": "semantic-release",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:once && npm run check-coverage"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
